def first_reverse_try(arr):
    if len(arr) > 0:
        firstindex = arr[0]
        lastindex = arr[-1]
        arr[0] = lastindex
        arr[-1] = firstindex

    return arr


#my attempt
def first_reverse_try(arr):
    for num in arr:
        temp = arr[-1]
        arr[-1] = arr[0]
        arr[0] = temp
    return arr
